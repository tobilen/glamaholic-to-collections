  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@jest/globals": "^29.7.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier"  : "^3.2.4"
  },
  "dependencies": {
    "yargs": "^17.0.1"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "/*.test.js",
      "/**/*.test.js"
